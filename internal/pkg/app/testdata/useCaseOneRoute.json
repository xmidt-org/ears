{
  "id": "u101",
  "userId": "boris",
  "name": "useCaseOneRoute",
  "receiver": {
    "plugin": "debug",
    "name": "useCaseRouteReceiver",
    "config": {
      "rounds": 1,
      "intervalMs": 10,
      "payload": {
        "Type": "Notification",
        "MessageId": "e684b540-9101-530b-9e85-ed705c063207",
        "TopicArn": "topicarn_ACCOUNT",
        "Message": "",
        "Timestamp": "2020-11-19T19:43:03.236Z",
        "SignatureVersion": "1",
        "Signature": "signature",
        "SigningCertURL": "http://123.pem",
        "UnsubscribeURL": "http://unsubscribe",
        "MessageAttributes": {
          "header.partner": {
            "Type": "String",
            "Value": "Partner"
          },
          "header.event": {
            "Type": "String",
            "Value": "CREATE"
          }
        }
      },
      "maxHistory": 100
    }
  },
  "sender": {
    "plugin": "debug",
    "name": "useCaseOneRouteSender",
    "config": {
      "destination": "stdout",
      "maxHistory": 100
    }
  },
  "filterChain": [
    {
      "plugin": "match",
      "name": "useCaseOneRouteMatcher",
      "config": {
        "mode": "allow",
        "matcher": "patternregex",
        "pattern": {
          "TopicArn": ".*_ACCOUNT$|.*_ACCOUNTPRODUCT$"
        }
      }
    },
    {
      "plugin": "decode",
      "name": "useCaseOneRouteDecoder",
      "config": {
        "fromPath": ".Message"
      }
    },
    {
      "plugin": "match",
      "name": "useCaseOneRouteMatcher",
      "config": {
        "mode": "allow",
        "matcher": "pattern",
        "pattern": {
          "Message": {
            "header": {
              "partner": "Partner",
              "type": "ACCOUNT_PRODUCT"
            }
          }
        }
      }
    },
    {
      "plugin": "transform",
      "name": "useCaseOneRouteTransformer",
      "config": {
        "transformation": {
          "message": {
            "op": "process",
            "uses": "xbo_account",
            "payload": {
              "metadata": {
                "type": "{.Message.header.type}",
                "event": "{.Message.header.event}",
                "timestamp": "{.Message.header.timestampMs}",
                "id": "{.Message.body.account.id}"
              },
              "body": {
                "addedAccountProducts": "{.Message.body.addedAccountProducts}",
                "removedAccountProducts": "{.Message.body.removedAccountProducts}"
              }
            }
          },
          "to": {
            "location": "{.Message.body.account.id}",
            "app": "myapp"
          }
        }
      }
    },
    {
      "plugin": "js",
      "name": "useCaseOneRouteFilterJs",
      "config": {
        "source": "if (_.event.payload.message.payload.body.addedAccountProducts.length == 0 && _.event.payload.message.payload.body.removedAccountProducts.length == 0) {\n  return null;\n} else {\n  return _.event;\n}"
      }
    },
    {
      "plugin": "hash",
      "name": "useCaseOneRouteHasher",
      "config": {
        "fromPath": ".to.location",
        "toPath": "metadata.kafka.partition",
        "hashAlgorithm": "fnv"
      }
    },
    {
      "plugin": "trace",
      "name": "useCaseOneRouteTracer",
      "config": {
        "tracePath" : ".tx.traceId"
      }
    },
    {
      "plugin": "log",
      "name": "useCaseOneRouteLogger",
      "config": null
    }
  ],
  "deliveryMode": "whoCares"
}
